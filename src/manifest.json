{
  "name": "Recent Bookmarks",
  "version": "1",
  "description": "List recent booksmarks",
  "permissions": [
    "bookmarks",
    "storage",
    "tabs",
    "chrome://favicon/"
  ],
  "background": {
    "scripts": [
      "faviconcacher.js"
    ]
  },
  "browser_action": {
    "default_title": "Recent Bookmarks",
    "default_icon": "icons/rating-32px.png",
    "default_popup": "popup.html",
    "theme_icons": [
      {
        "light": "icons/rating-dark-16px.png",
        "dark": "icons/rating-16px.png",
        "size": 16
      },
      {
        "light": "icons/rating-dark-32px.png",
        "dark": "icons/rating-32px.png",
        "size": 32
      },
      {
        "light": "icons/rating-dark-48px.png",
        "dark": "icons/rating-48px.png",
        "size": 48
      },
      {
        "light": "icons/rating-dark-64px.png",
        "dark": "icons/rating-64px.png",
        "size": 64
      }
    ]
  },
  "icons": {
    "16": "icons/rating-16px.png",
    "32": "icons/rating-32px.png",
    "48": "icons/rating-48px.png",
    "64": "icons/rating-64px.png",
    "96": "icons/rating-96px.png",
    "128": "icons/rating-128px.png"
  },
  "manifest_version": 2
}
